{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build:client",
  "outputDirectory": "dist/public",
  "installCommand": "pnpm install",
  "framework": "vite",
  "functions": {
    "api/[...route].ts": {
      "runtime": "@vercel/node@5.5.16"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/[...route]"
    }
  ]
}
